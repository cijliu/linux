		gpio_chip9: gpio_chip@120d9000 {
			compatible = "arm,pl061", "arm,primecell";
			reg = <0x120d9000 0x1000>;
			interrupts = <0 25 4>;
			clocks = <&clock  HI3559V200_SYSAPB_CLK>;
			clock-names = "apb_pclk";
			#gpio-cells = <2>;
			status = "disabled";
		};

		gpio_chip10: gpio_chip@120da000 {
			compatible = "arm,pl061", "arm,primecell";
			reg = <0x120da000 0x1000>;
			interrupts = <0 26 4>;
